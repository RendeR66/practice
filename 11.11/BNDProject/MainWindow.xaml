        xmlns:local="clr-namespace:BNDProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="266*"/>
            <RowDefinition Height="153*"/>
        </Grid.RowDefinitions>
        <Button x:Name="butt" Content="уравнение 1" HorizontalAlignment="Left" Margin="106,10,0,0" VerticalAlignment="Top" Width="150" Click="Button_Click" Height="50"/>
        <Button x:Name="cnop" Content="уравнение 2" HorizontalAlignment="Left" Margin="492,0,0,0" VerticalAlignment="Top" Width="150" Height="50" RenderTransformOrigin="0.5,0.5" Click="Button_Click_1" IsCancel="True"/>
