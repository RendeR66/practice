        <Label x:Name="yraww" Content="ответ уравнения 2" HorizontalAlignment="Left" Margin="492,65,0,0" VerticalAlignment="Top" Height="35" Width="150"/>
        <Button x:NSame="buttt" Content="Button" HorizontalAlignment="Left" Height="50" Margin="106,148,0,0" VerticalAlignment="Top" Width="150" Click="buttt_Click"/>
        <Button x:Name="butttt" Content="Button" HorizontalAlignment="Left" Height="50" Margin="492,148,0,0" VerticalAlignment="Top" Width="150" Click="butttt_Click"/>
        <Label x:Name="yrawww" Content="ответ уравнния 3" HorizontalAlignment="Left" Margin="106,203,0,0" VerticalAlignment="Top" Height="35" Width="150"/>
        <Label x:Name="yrawwww" Content="ответ уравнния 4" HorizontalAlignment="Left" Margin="492,203,0,0" VerticalAlignment="Top" Height="35" Width="150"/>

    </Grid>
</Window>
